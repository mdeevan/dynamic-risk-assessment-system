name: data_ingestion
python_env: virtualenv

entry_points:
  main:
    parameters:

      in_folder:
        description: in folder 
        type: string

       in_filename:
        description: in filename
        type: string

       out_folder:
        description: out folder
        type: string

       out_filename:
        description: out filename
        type: string


    command: >-
        python run_data_ingestion.py  --in_folder {in_folder}  -- in_filename { in_filename}  -- out_folder { out_folder}  -- out_filename { out_filename} 
